<Window x:Class="GradeMeUp.DeleteAssignmentDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GradeMeUp"
        mc:Ignorable="d"
        Title="Delete Assignment" Height="179" Width="443.167">
    <Grid  HorizontalAlignment="Left" VerticalAlignment="Center" Width="433" Height="202" Margin="0,0,0,-1">
        <TextBlock x:Name="textBlock" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center" Text="Are you sure you wish to delete the following student?" Width="390" Margin="33,57,10,130"/>
        <Button x:Name="StudentDeleteOK" Content="OK" HorizontalAlignment="Left" Margin="100,127,0,0" VerticalAlignment="Top" Width="75" Click="StudentDeleteOK_Click"/>
        <Button x:Name="StudentDeleteCancel" Content="Cancel" HorizontalAlignment="Left" Margin="240,127,0,0" VerticalAlignment="Top" Width="76"/>
        <TextBlock x:Name="StudentDeleteName" HorizontalAlignment="Center" TextAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap" Text="AssignmentName" Width="302" Margin="54,84,77,103"/>
    </Grid>
</Window>
